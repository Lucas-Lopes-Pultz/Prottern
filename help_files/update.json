{
    "name": "update",
    "about": "Updates a template from folder",
    "parents": ["tp"],
    "args": {
        "inputs": [
            {
                "name": "temp-name",
                "help": "Sets the template name",
                "required": true
            },
            {
                "name": "folder-path",
                "help": "Sets the the input folder path",
                "default_value": "./"
            }
        ]
    }
}